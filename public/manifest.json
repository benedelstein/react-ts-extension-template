{
  "name": "hbd facebook export",
  "description": "add your facebook birthdays to hbd!",
  "default_locale": "en",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icons/icon.16.png",
    "32": "icons/icon.32.png",
    "48": "icons/icon.48.png",
    "64": "icons/icon.64.png",
    "128": "icons/icon.128.png",
    "256": "icons/icon.256.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*//*facebook.*/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
